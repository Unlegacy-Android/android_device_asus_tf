<!-- Mixer configuration file for TF201/TF300TG/TF300TL/TF700T -->

<mixer>
  <!-- These are the initial mixer settings -->
  <ctl name="Int Spk Switch" value="0" />
  <ctl name="Headphone Jack Switch" value="0" />
  <ctl name="AUX Switch" value="0" />
  <ctl name="Recording Gain" val="0"/>
  <ctl name="DMIC Switch" val="0" />
  <ctl name="Mic Jack Switch" val="0" />
  <ctl name="Int Mic Switch" val="0" />

  <path name="speaker">
    <ctl name="Int Spk Switch" value="1" />
  </path>

  <path name="headphone">
    <ctl name="Headphone Jack Switch" value="1" />
  </path>

  <path name="dock">
    <ctl name="AUX Switch" value="1" />
  </path>

  <path name="builtin-mic">
    <ctl name="Recording Gain" val="0" />
    <ctl name="DMIC Switch" val="1" />
    <ctl name="Int Mic Switch" val="1" />
  </path>

  <path name="headset-mic">
    <ctl name="Recording Gain" val="1" />
    <ctl name="DMIC Switch" val="0" />
    <ctl name="Mic Jack Switch" value="1" />
  </path>
</mixer>
