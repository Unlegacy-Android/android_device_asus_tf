<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
     package="com.unlegacy.settings.device"
     android:sharedUserId="android.uid.system"
      android:versionCode="2"
      android:versionName="2.0">

    <uses-sdk android:minSdkVersion="18" android:targetSdkVersion="18"/>

    <uses-permission android:name="com.unlegacy.asusdec.permission.KEYPAD_RECEIVER" />

    <application
        android:label="@string/app_name"
        android:icon="@mipmap/ic_launcher_settings">

        <receiver
            android:name=".DeviceBroadcastReceiver"
            android:exported="false">
            <intent-filter>
                <action android:name="com.unlegacy.asusdec.actions.ACTION_DOCK_KEYPAD_KEY_PRESSED" />
            </intent-filter>
        </receiver>

    </application>

</manifest>
